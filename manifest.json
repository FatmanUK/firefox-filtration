{
  "manifest_version": 2,
  "name": "Firefox Filtration",
  "version": "0.1",
  "author": "Adam J. Richardson",
  "description": "Filter and auto-tag your bookmarks",
  "icons": {
    "48": "icons/icon48.jpg"
  },
  "permissions": ["contextMenus"],
  "background" : {
    "scripts": ["background.js"]
  },
  "browser_action" : {
    "default_icon": {
      "48": "icons/icon48.jpg"
    },
    "default_title": "Firefox Filtration"
  }
}
